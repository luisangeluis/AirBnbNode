{
  "openapi": "3.0.1",
  "info": {
    "title": "API Rest Clon de Airbnb",
    "description": "Api para imitar las funciones de la pagina Airbnb",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url":"http://localhost:3000",
      "'description":"una url"
    },
    {
      "url":"https://airbnb-clone.onrender.com",
      "'description":"https://airbnb-clone.onrender.com"
    }
  ],
  "paths": {
    "/api/v1/users": {
      "get": {
        "description": "Get all mi users",
        "tags": [
          "Users"
        ],
        "responses": {
          "200": {
            "description": "We successfully get all users"
          },
          "400": {
            "description": "We get an error if the request is not resolved"
          }
        }
      }
    },
    "/api/v1/users/{id}": {
      "get": {
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID del usuario",
            "required": true,
            "type": "string"
          }
        ],
        "description": "Get one user by Id",
        "tags": [
          "Users"
        ],
        "responses": {
          "200": {
            "description": "We successfully get one user"
          },
          "400": {
            "description": "We get an error if the request is not resolved"
          }
        }
      }
    }
  }
}